{
  "mcpServers": {
    "swic": {
      "command": "bun",
      "args": [
        "run",
        "dist/server/Server.js"
      ]
    },
    "swic-bun": {
      "command": "bun",
      "args": [
        "run",
        "src/server/Server.ts"
      ]
    },
    "swic-npm": {
      "command": "npx",
      "args": [
        "-y",
        "swic-mcp"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/paulkelcey/Dev"
      ]
    }
  }
}
